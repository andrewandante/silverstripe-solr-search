{
  "name": "solr-search",
  "description": "Searching done right",
  "version": "1.0",
  "private": false,
  "license": "BSD-3-Clause",
  "directories": {
    "test": "tests"
  },
  "bin": {
    "webpack": "./node_modules/.bin/webpack",
    "sass-lint": "./node_modules/.bin/sass-lint"
  },
  "engines": {
    "node": "^10.x"
  },
  "devDependencies": {
    "@silverstripe/eslint-config": "^0.0.5",
    "@silverstripe/webpack-config": "^1.0.0",
    "laravel-mix": "^4.1.2",
    "sass": "^1.22.10",
    "sass-loader": "^7.3.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^2",
    "webpack-bundle-analyzer": "^3.4.1"
  },
  "scripts": {
    "build": "NODE_ENV=development ./node_modules/.bin/webpack-cli --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "yarn build --watch",
    "package": "NODE_ENV=production ./node_modules/.bin/webpack-cli --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "lint": "eslint client/src && sass-lint -v",
    "js-fix": "eslint client/src --fix",
    "bundle": "yarn && NODE_ENV=production webpack -p --bail --progress"
  },
  "dependencies": {
    "deep-freeze-strict": "^1.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "window-or-global": "^1.0.1"
  },
  "resolutions": {
    "eslint": "^4.6.1"
  },
  "babel": {
    "presets": [
      "react",
      "env"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  }
}
